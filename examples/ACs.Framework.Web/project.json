{
  "version": "1.0.0-*",
  "userSecretsId": "ACs-Framework-Example",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final",
    "Microsoft.Extensions.Configuration.FileProviderExtensions": "1.0.0-rc1-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc1-final",
    "Microsoft.Extensions.Logging": "1.0.0-rc1-final",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc1-final",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-rc1-final",
    "Microsoft.AspNet.Diagnostics": "1.0.0-rc1-final",
    "Microsoft.Extensions.SecretManager": "1.0.0-rc1-final",
    "NHibernate": "4.0.4.4000",
    "FluentNHibernate": "2.0.3",
    "ACs.NHibernate": "",
    "ACs.NHibernate.Next": "",
    "ACs.NHibernate.Generic": "",
    "ACs.Angular.Next": "",
    "ACs.Net.Mail": "",
    "FluentMigrator": "1.6.1",
    "FluentMigrator.Runner.DNX": "1.0.0-*"
  },


  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5000/tiregauge",
    "migrate": "FluentMigrator.Runner.DNX --provider sqlserver2008 --connectionString Server=.\\sqlexpress;Database=Foo;Trusted_Connection=True;MultipleActiveResultSets=true --output migrator.sql",
    "migrate-down": "FluentMigrator.Runner.DNX --provider sqlserver2008 --connectionString Server=.\\sqlexpress;Database=Foo;Trusted_Connection=True;MultipleActiveResultSets=true --output migrator-down.sql --task migrate:down"
  },

  "frameworks": {
    "dnx451": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ]
}
